<table class="transcript">
  <tr class='head'>
    <th colspan="5">TRANSCRIPT</th>
  </tr>
  <tr class='subhead' >
    <td>Year</td><td>Course</td><td>Course Name</td><td>Grade</td><td align='right'>Delete?</td>
  </tr>
  <% @user.transcript.each do |c| %>
  <tr class='item'>
    <td><%= c.year %></td>
    <td><%= c.dept + c.number %></td>
    <td><%= c.name %></td>
    <td><%= c.grade %></td>
    <td align='right'>


  </tr>
  <% end %>
  <tr class='subhead'>
    <td colspan='4' align='right'>Credit Taken</td><td align='center'>not yet!</td>
  </tr>
  <tr class='subhead'>
    <td colspan='4' align='right'>GPA</td><td align='center'>not yet!</td>
  </tr>

</table>

<br />
<br />

<div >
   <%= render 'uploads/uploadTranscript' %>
</div>

<div >
   <%= render 'uploads/uploadCourse' %>
</div>

<p>
  * Please only upload Unofficial UMass transcirpts,<br>
  found on your Spire account: Spire->Menu->Academics->Unofficial Transcript
</p>
